    "import json\n",
    "import pprint\n",
    "import requests\n",
    "results = []\n",
    "num_tests = 3\n",
    "# File contains the following:\n",
    "# - Name of service \n",
    "# - Full text of all documents\n",
    "# - All points associated with service\n",
    "# Each of these attributes is stored in a JSON object, 1 per line \n",
    "# Only have like 30-40 since they are so big\n",
    "with open(\"data.txt\", \"r\") as f:\n",
    "    line = f.readline()\n",
    "    lc = 1\n",
    "    while line and lc < num_tests:\n",
    "        test = json.loads(line)\n",
    "        # Insert documents into database\n",
    "        for doc in test[\"documents\"]:\n",
    "            response = requests.post(\n",
    "                \"http://127.0.0.1:8000/add\",\n",
    "                json={\n",
    "                    \"service\": test[\"name\"],\n",
    "                    \"url\": doc[\"url\"],\n",
    "                    \"name\": doc[\"name\"],\n",
    "                    \"text\": doc[\"text\"]\n",
    "                }\n",
    "            )\n",
    "        line = f.readline()\n",
    "        lc += 1"
   ]
